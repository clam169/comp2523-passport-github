<h1 class="mt-4">Dashboard</h1>
<p class="lead mb-3">Welcome <%= user.name %></p>
<p>Current Active Sessions: <%= sessions.length %><br>
  <% let userId; for(let session of Object.keys(sessions)) {
    userId = JSON.parse(sessions[session])["passport"]["user"]; %>
    <p>SessionID <%= session %> <br>
      UserID: <%= userId %> </p>
      <a href="/admin/revoke/<%= session %>">Revoke Session</a>
      <br>
  <% } %>
